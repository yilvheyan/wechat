/*   小气球动画*/

.homebigimg {
    position: absolute;
    right: 8%;
    width: 170rpx;
    z-index: 100;
    pointer-events: none;
    height: calc(100% - 250rpx);
    animation-name: myfirst; /*动画的名称 */
    animation-duration: 2000ms; /*动画从开始到结束的时间*/
    animation-timing-function: linear; /*动画执行快慢的参数*/
    animation-iteration-count: infinite; /*动画执行多少次的参数*//*以下是兼容ios所需，参数意义与上相同*/
    -webkit-animation-name: myfirst;
    -webkit-animation-duration: 2000ms;
    -webkit-animation-timing-function: linear;
    -webkit-animation-iteration-count: infinite;
}

@keyframes myfirst {
    0% {
        transform: translate(8%, 220%);
    }

    50% {
        transform: translate(8%, 210%);
    }

    100% {
        transform: translate(8%, 220%);
    }
}

.homesmallimg {
    position: absolute;
    right: 8%;
    width: 70rpx;
    z-index: 100;
    pointer-events: none;
    height: calc(100% - 550rpx);
    animation-name: homesmallimgll; /*动画的名称 */
    animation-duration: 2000ms; /*动画从开始到结束的时间*/
    animation-timing-function: linear; /*动画执行快慢的参数*/
    animation-iteration-count: infinite; /*动画执行多少次的参数*//*以下是兼容ios所需，参数意义与上相同*/
    -webkit-animation-name: homesmallimgll;
    -webkit-animation-duration: 2000ms;
    -webkit-animation-timing-function: linear;
    -webkit-animation-iteration-count: infinite;
}

@keyframes homesmallimgll {
    0% {
        transform: translate(8%, 250%);
    }

    50% {
        transform: translate(8%, 240%);
    }

    100% {
        transform: translate(8%, 250%);
    }
}
