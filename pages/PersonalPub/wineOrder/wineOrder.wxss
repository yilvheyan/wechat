.wineorder {
  width: 750rpx;
  height: 1206rpx;
  background: rgba(238, 238, 238, 1);
  overflow: hidden;
}

/* 去除滚动条样式 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* <!-- 姓名电话售后地址样式 --> */

.wineorder_site_top {
  width: 706rpx;
  height: 148rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 15rpx;
  margin: 20rpx 0rpx 0rpx 22rpx;
  overflow: hidden;
}

.wineorder_site_top_view {
  width: 652rpx;
  height: 90rpx;
  margin: 39rpx 0rpx 0rpx 29rpx;
  display: flex;
}

.wineorder_site_top_view_img {
  width: 70rpx;
  height: 70rpx;
}

.wineorder_site_two {
  width: 500rpx;
  margin-left: 34rpx;
  display: flex;
  flex-wrap: nowrap;
}

.wineorder_site_two_name {
  font-size: 32rpx;
  width: 300rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 45rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.wineorder_site_two_phone {
  width: 200rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 55rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.wineorder_site_two_map {
  width: 500rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  margin: -30rpx 0rpx 0rpx 120rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.wineorder_site_top_z {
  width: 19rpx;
  height: 33rpx;
  margin: 20rpx;
}

/* <!-- 商品信息样式 --> */

.wineorder_shop {
  width: 707rpx;
  height: 548rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 15rpx;
  margin: 20rpx 0rpx 0rpx 22rpx;
}

.shop_message {
  width: 100%;
  height: 91rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 91rpx;
  border-bottom: 1rpx #f2f2f2 solid;
}

.shop_message span {
  margin-left: 34rpx;
}

.shop_message_style {
  width: 100%;
  height: 218rpx;
  /* border: 1rpx saddlebrown solid; */
}

.shop_message_style_one {
  display: flex;
  width: 678rpx;
  height: 158rpx;
  /* border: 1rpx saddlebrown solid; */
  margin-top: 30rpx;
  margin-left: 23rpx;
}

.shop_message_style_one image {
  width: 158rpx;
  height: 158rpx;
}

.shop_message_style_two {
  width: 480rpx;
  height: 158rpx;
  /* border: 1rpx saddlebrown solid; */
  margin-left: 22rpx;
}

.shop_message_style_top {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.shop_message_style_bttom {
  font-size: 26rpx;
  font-family: PingFangSC-Medium;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  margin-top: 23rpx;
}

.shop_message_style_zzz {
  font-size: 24rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: gray;
  margin-top: 10rpx;
}

