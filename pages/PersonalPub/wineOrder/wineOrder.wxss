/* <!-- 支付样式 --> */

.wineorder_pay {
  width: 750rpx;
  height: 98rpx;
  background: rgba(255, 255, 255, 1);
  position: fixed;
  bottom: 0rpx;
  display: flex;
}

.wineorder_pay_one {
  margin-left: 49rpx;
}

.wineorder_pay_col {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 98rpx;
}

.wineorder_pay_row {
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  font-weight: bold;
  color: #fa3232;
  line-height: 98rpx;
}

.wineorder_pay_left {
  width: 500rpx;
  height: 98rpx;
}

.wineorder_pay_right {
  width: 250rpx;
  height: 98rpx;
  background: linear-gradient(159deg, rgba(116, 202, 251, 1) 0%, rgba(91, 163, 237, 1) 100%);
  border-radius: none;
  font-size: 34rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 98rpx;
}

.createOrdercla {
  pointer-events: none;
  background: gainsboro;
}
