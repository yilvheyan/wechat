/* 修改皮肤弹出底部弹框 */

.pupContentBG {
    width: 100%;
    /* height: 705rpx; */
    height: 100%;
    position: fixed;
    top: 0;
    z-index: 100;
}

.pupContent {
    width: 100%;
    /* top:1252rpx; */
    left: 0;
    bottom: 0;
    position: fixed;
    z-index: 999;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px -4rpx 24rpx 0rpx rgba(152, 173, 197, 0.4);
    border-radius: 12rpx 12rpx 0px 0px;
}

/* 设置显示的背景 */

.showBG {
    z-index: 888;
    display: block;
}

.hideBG {
    display: none;
}

/* 弹出或关闭动画来动态设置内容高度 */

@keyframes slideBGtUp {
    from {
        background: transparent;
    }

    to {
        /* background: rgba(0, 0, 0, 0.1); */
        background: black;
        opacity: 0.5;
    }
}

@keyframes slideBGDown {
    from {
        background: rgba(0, 0, 0, 0.1);
    }

    to {
        background: transparent;
    }
}

/* 显示或关闭内容时动画 */

.openBG {
    animation: slideBGtUp 0.11s ease-in both;
}

.closeBG {
    animation: slideBGDown 0.1s ease-in both;
}

/* 设置显示内容 */

.showContent {
    display: block;
}

.hideContent {
    display: none;
}

