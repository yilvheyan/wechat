page {
    height: 100%;
}

.GoodsOrder {
    width: 750rpx;
    height: 100%;
    position: relative;
    background: rgba(243, 246, 250, 1);
    overflow: hidden;
}

.GoodsOrder_view {
    width: 707rpx;
    height: 373rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 12rpx;
    margin: 19rpx 22rpx 0rpx 22rpx;
    overflow: hidden;
}

.GoodsOrder_view_one {
    font-size: 32rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(51, 51, 51, 1);
    margin: 22rpx 0rpx 0rpx 33rpx;
    height: 91rpx;
    line-height: 91rpx;
    border-bottom: 1rpx solid #f2f2f2;
}

.GoodsOrder_view_xx {
    width: 704rpx;
    height: 200rpx;
    display: flex;
}

.GoodsOrder_view_xx_img {
    width: 201rpx;
    height: 200rpx;
    margin: 30rpx 0rpx 0rpx 19rpx;
}

.GoodsOrder_view_right {
    width: 500rpx;
    height: 200rpx;
    margin-left: 33rpx;
}

.GoodsOrder_view_right_top {
    height: 80rpx;
    font-size: 26rpx;
    font-family: PingFangSC;
    font-weight: 500;
    color: rgba(51, 51, 51, 1);
    margin-top: 41rpx;
}

.GoodsOrder_view_right_bottom {
    width: 100%;
    height: 80rpx;
    font-size: 22rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    margin-top: 38rpx;
    display: flex;
    align-items: center;
}

.withdrawDetails_center_one_span {
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 500;
    color: rgba(67, 147, 242, 1);
}

.GoodsOrder_bottom {
    width: 750rpx;
    height: 98rpx;
    background: rgba(255, 255, 255, 1);
    position: fixed;
    bottom: 0rpx;
}

.GoodsOrder_bottom_left {
    width: 500rpx;
    height: 98rpx;
    line-height: 98rpx;
    float: left;
}

.GoodsOrder_bottom_left_heji {
    margin-left: 49rpx;
    font-size: 28rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
}

.GoodsOrder_bottom_left_span {
    color: #4393f2;
    font-size: 36rpx;
    line-height: 98rpx;
    margin-left: 30rpx;
}

.GoodsOrder_bottom_right {
    width: 250rpx;
    height: 98rpx;
    background: rgba(67, 147, 242, 1);
    text-align: center;
    line-height: 98rpx;
    float: right;
    font-size: 34rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
}

/* 遮罩 */

.zhezao {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0px;
    background: rgba(0, 0, 0, 0.4);
    overflow: hidden;
}

.tanchu_view {
    width: 80%;
    height: 467rpx;
    margin: 50% auto;
    overflow: hidden;
    background: rgba(255, 255, 255, 1);
    border-radius: 10rpx;
    padding: 4rpx;
}

.tanchu_view_cuowu_list {
    text-align: right;
}

.tanchu_view_cuowu {
    width: 48rpx;
    height: 48rpx;
    margin-right: 10rpx;
}

.tanchu_view_que {
    width: 102rpx;
    height: 102rpx;
    margin: 0rpx 0rpx 0rpx 249rpx;
}

.tanchu_view_succ {
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(51, 51, 51, 1);
    text-align: center;
    margin-top: 12rpx;
}

.tanchu_view_yea {
    font-size: 24rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    text-align: center;
    margin-top: 25rpx;
}

.tanchu_view_goto {
    font-size: 24rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(41, 139, 241, 1);
