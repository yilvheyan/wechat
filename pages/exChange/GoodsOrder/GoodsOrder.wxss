    line-height: 97rpx;
    text-align: center;
    border-top: 1rpx solid rgba(238, 238, 238, 1);
}

/*主容器*/

.stepper {
    width: 202rpx;
    height: 46rpx;
    background: rgba(255, 255, 255, 1);
    border: 1px solid rgba(224, 223, 230, 1);
    border-radius: 4rpx;
    margin-left: 80rpx;
}

/* 份额 */

.share {
    width: 53rpx;
