.lunbo-image {
    width: 750rpx;
    height: 480rpx;
    border-top: 1rpx #c5cbd9 solid;
}

.swiper_img {
    width: 100%;
    height: 480rpx;
}

.withdrawDetails_slect {
    width: 750rpx;
    /* border: 1rpx #c5cbd9 solid; */
}

.withdrawDetails_center {
    margin-left: 48rpx;
    width: 667rpx;
    height: 286rpx;
}

.withdrawDetails_center_one {
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
}

.withdrawDetails_center_one_span {
    font-size: 50rpx;
    font-family: PingFangSC;
    font-weight: 500;
    color: rgba(67, 147, 242, 1);
}

.withdrawDetails_center_two {
    font-size: 34rpx;
    font-family: PingFangSC-Medium;
    font-weight: bold;
    color: rgba(51, 51, 51, 1);
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.withdrawDetails_center_three {
    font-size: 30rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    line-height: 42rpx;
    margin-top: 17rpx;
}

.withdrawDetails_details {
    width: 750rpx;
    height: 120rpx;
    background: rgba(245, 246, 247, 1);
    overflow: hidden;
    display: flex;
}

.withdrawDetails_details_one {
    width: 7rpx;
    height: 30rpx;
    background: rgba(204, 204, 204, 1);
    border-radius: 1rpx;
    margin: 37rpx 0rpx 0rpx 55rpx;
}

.withdrawDetails_details_two {
    font-size: 34rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    margin: 27rpx 0rpx 0rpx 8rpx;
}

.withdrawDetails_bg {
    width: 750rpx;
}

.withdrawDetails_foot {
    width: 640rpx;
    height: 76rpx;
    background: rgba(67, 147, 242, 1);
    border-radius: 6rpx;
    font-size: 28rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
    line-height: 76rpx;
    text-align: center;
    position: fixed;
    bottom: 11rpx;
    left: 55rpx;
}
